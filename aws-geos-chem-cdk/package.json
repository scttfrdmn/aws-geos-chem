{
  "name": "aws-geos-chem-cdk",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for GEOS-Chem AWS Cloud Runner",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:core": "cdk deploy geos-chem-core-infra",
    "deploy:compute": "cdk deploy geos-chem-compute",
    "deploy:data": "cdk deploy geos-chem-data",
    "deploy:web": "cdk deploy geos-chem-web-app",
    "deploy:dev": "node scripts/deploy.js dev",
    "deploy:test": "node scripts/deploy.js test",
    "deploy:prod": "node scripts/deploy.js prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "cleanup": "node scripts/cleanup.js",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:ts": "eslint --ext .ts .",
    "lint:js": "eslint --ext .js .",
    "lint:fix": "eslint --ext .ts,.js --fix .",
    "lint:py": "flake8",
    "lint:bash": "find .. -type f -name \"*.sh\" -exec shellcheck {} \\;",
    "lint:all": "npm run lint && npm run lint:py && npm run lint:bash"
  },
  "keywords": ["aws", "cdk", "geos-chem", "cloud"],
  "author": "AWS GEOS-Chem Cloud Runner Contributors",
  "license": "Apache-2.0",
  "type": "commonjs",
  "dependencies": {
    "@types/node": "^22.15.17",
    "aws-cdk-lib": "^2.195.0",
    "constructs": "^10.4.2",
    "typescript": "^5.8.3",
    "source-map-support": "^0.5.21",
    "dotenv": "^16.3.1",
    "aws-sdk": "^2.1445.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "aws-cdk": "^2.115.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}